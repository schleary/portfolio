<h1>
	<%=link_to @project.title,  "http://#{@project.link}", target: "_blank"%><br>
</h1>
<%=link_to @project.link,  "http://#{@project.link}", target: "_blank"%><br>
<%=link_to @project.image,  "http://#{@project.link}", target: "_blank"%><br>
<br>
<%= @project.description %><br>
 
<%= link_to 'All Projects', projects_path %>
<% if user_signed_in? %>
	<%= link_to 'Edit', edit_project_path(@project) %>
	<%= link_to 'Delete', project_path(@project), method: :delete,
	    	data: { confirm: 'Are you sure?' } %>
	<%= link_to 'New Project', new_project_path %>
<% end %>