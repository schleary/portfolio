<%= @project.image %><br>
<%= @project.title %><br>
<%= @project.link %><br>
<%= @project.description %><br>
 
<% if user_signed_in? %>
	<%= link_to 'Edit', edit_project_path(@project) %>
	<%= link_to 'Delete', project_path(@project), method: :delete,
        	data: { confirm: 'Are you sure?' } %>
	<%= link_to 'All Projects', projects_path %>
<% end %>