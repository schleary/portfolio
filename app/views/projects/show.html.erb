
<p>
  <strong>Image:</strong>
  <%= @project.image %>
</p>

<p>
  <strong>Title:</strong>
  <%= @project.title %>
</p>

<p>
  <strong>Link:</strong>
  <%= @project.link %>
</p>
 
<p>
  <strong>Description:</strong>
  <%= @project.description %>
</p>
 
<% if user_signed_in? %>
	<%= link_to 'Edit', edit_project_path(@project) %>
	<%= link_to 'Delete', project_path(@project), method: :delete,
        	data: { confirm: 'Are you sure?' } %>
	<%= link_to 'All Projects', projects_path %>
<% end %>